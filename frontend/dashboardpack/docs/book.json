{
  "title": "Admindek VanillaJS Documentation",
  "description": "Comprehensive documentation for Admindek VanillaJS admin dashboard template",
  "author": "DashboardPack",
  "language": "en",
  "direction": "ltr",
  "gitbook": ">=3.0.0",
  "root": "./",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search",
    "anchors",
    "edit-link",
    "github",
    "copy-code-button",
    "prism",
    "-highlight",
    "expandable-chapters",
    "sitemap-general",
    "page-toc-button",
    "back-to-top-button"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/your-username/Admindek-VanillaJS/edit/main/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/your-username/Admindek-VanillaJS"
    },
    "sitemap-general": {
      "prefix": "https://your-docs-domain.gitbook.io/admindek/"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    }
  },
  "variables": {
    "version": "3.1.0",
    "template_name": "Admindek VanillaJS",
    "company": "DashboardPack",
    "website": "https://dashboardpack.com",
    "support_email": "dashboardpack@gmail.com"
  }
}